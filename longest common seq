#longest common sequence 
xyz = ["a","a","b"]
abc = ["a","a","a","a","a","b"]
luc = []
fullstr=[]
for k in range(len(xyz)):
    x=0
    for i in range(k,len(xyz)):
        for j in range(x,len(abc)):
            if xyz[i]== abc[j]:
                luc.append(xyz[i])
                x= j
                break
    fullstr.append(luc)
    luc=[] #for all availabe seq match
   # break #for longest match
print(fullstr)
